{
  "theme": "One Dark",
  "autosave": "on_focus_change",
  "format_on_save": "on",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "ui_font_family": "Cascadia Code NF",
  "buffer_font_family": "Cascadia Code NF",
  "languages": {
    "Python": {
      "language_servers": [
        "ruff"
      ]
    }
  },
  "terminal": {
    "option_as_meta": false
  },
  "auto_install_extensions": {
    "html": true,
    "fish": true,
    "toml": true
  }
}
